<h1>Listing fostereds</h1>

<table>
  <thead>
    <tr>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @fostereds.each do |fostered| %>
      <tr>
        <td><%= link_to 'Show', fostered %></td>
        <td><%= link_to 'Edit', edit_fostered_path(fostered) %></td>
        <td><%= link_to 'Destroy', fostered, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fostered', new_fostered_path %>
